    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "mocha": "^6.0.2",
    "protractor": "~5.4.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tslint": "~5.13.1",
    "typescript": "~3.7.5",
    "webpack-cli": "^3.2.3",
    "zone.js": "~0.10.3"
  }
}
