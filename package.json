{
  "name": "awesome-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:docs",
    "start": "ng serve --port=4202",
    "build:docs": "ts-node scripts/build.ts",
    "watch": "chokidar './src/assets/content/**/*.md' --initial -c 'npm run build:docs || true'",
    "prebuild": "npm run build:docs",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run awesome-angular:serve-ssr",
    "serve:ssr": "node dist/awesome-angular-server/main.js",
    "build:ssr": "ng build --prod && ng run awesome-angular:server:production",
    "prerender": "ng run awesome-angular:prerender",
    "graphql-codegen": "graphql-codegen --config src/codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ts-node ./scripts/pre-commit.ts"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@angular/service-worker": "^9.1.9",
    "@nguniversal/express-engine": "^9.0.0-rc.0",
    "apollo-angular": "^1.9.1",
    "apollo-angular-link-http": "^1.10.0",
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-link": "^1.2.11",
    "core-js": "^2.6.5",
    "date-fns": "^2.14.0",
    "express": "^4.15.2",
    "github-slugger": "^1.2.1",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.12.6",
    "hash.js": "^1.1.7",
    "highlight.js": "^9.15.6",
    "lodash": "^4.17.15",
    "marked": "^0.6.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@angular/platform-server": "^9.1.9",
    "@awesome-fe/prerender": "0.0.5",
    "@graphql-codegen/cli": "^1.14.0",
    "@graphql-codegen/typescript-apollo-angular": "^1.14.0",
    "@graphql-codegen/typescript-operations": "^1.14.0",
    "@nguniversal/builders": "^9.0.0-rc.0",
    "@nguniversal/express-engine": "^9.0.0-rc.0",
    "@types/chokidar": "^1.7.5",
    "@types/express": "^4.17.0",
    "@types/glob": "^7.1.1",
    "@types/highlight.js": "^9.12.3",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.6",
    "@types/lodash": "^4.14.121",
    "@types/marked": "^0.6.1",
    "@types/node": "^12.11.1",
    "chai": "^4.2.0",
    "chokidar": "^2.1.2",
    "chokidar-cli": "^1.2.2",
    "codelyzer": "^5.1.2",
    "cross-spawn": "^6.0.5",
    "glob": "^7.1.3",
    "http-server-spa": "^1.3.0",
